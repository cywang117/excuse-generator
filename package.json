{
  "name": "rpt21-mvp",
  "version": "1.0.0",
  "description": "Hack Reactor's MVP sprint - an app idea generator",
  "scripts": {
    "server-dev": "nodemon server/index.js",
    "client-dev": "concurrently \"webpack --mode development -w -d\" \"live-server client/dist --watch=client --wait=500 --ignore=node_modules\" --kill-others",
    "start": "node server/index.js",
    "postinstall": "cd server && npm install && cd ../client && npm install && cd .. && webpack -p"
  },
